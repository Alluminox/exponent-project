{
  "name": "exponent-layout",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack",
    "start:design": "npm-run-all --parallel start:server watch:sass ",
    "start:server": "lite-server -c lite-server.json",
    "watch:sass": "node-sass src/sass/main.scss src/css/style.css -w",
    "compile:css": "node-sass src/sass/main.scss src/css/compress.css ",
    "input": "css/compress.css",
    "output": "css/prefix.css",
    "prefix:config": "postcss --config postcss.json",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" src/css/compress.css -o src/css/prefix.css",
    "compress:css": "node-sass src/css/prefix.css src/css/style.min.css --output-style=compressed",
    "build:css": "npm-run-all compile:css prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "jquery": "^3.4.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "autoprefixer": "^7.2.6",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concat": "^1.0.3",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "lite-server": "^2.5.4",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^4.1.1",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.2",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
